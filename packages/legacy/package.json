{
  "name": "supermouse",
  "version": "2.0.4",
  "description": "The all-in-one bundle for Supermouse.js. Includes Core, Dot, and Ring plugins with v1 API compatibility.",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "O.S David",
  "url": "https://github.com/Whitestar14/supermouse-js",
  "license": "MIT",
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "@supermousejs/core": "workspace:*",
    "@supermousejs/dot": "workspace:*",
    "@supermousejs/ring": "workspace:*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  }
}